{
  "name": "dva-helper",
  "displayName": "dva-helper",
  "description": "dva helper make you write code happy",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.helloWorld"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "commit": "git-cz",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test-local": " egg-bin test",
    "cov": " egg-bin cov"
  },
  "pre-commit": [],
  "egg": {
    "typescript": true
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "@types/power-assert": "^1.5.0",
    "commitizen": "^3.1.1",
    "conventional-changelog-cli": "^2.0.17",
    "egg-bin": "^4.13.1",
    "pre-commit": "^1.2.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  }
}
